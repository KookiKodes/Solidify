{
  "name": "@solidifront/vite-generate-shopify-locales",
  "version": "1.0.0",
  "private": false,
  "description": "Zero-config vite plugin that fetches and generates you're Storefront's shop locales",
  "license": "MIT",
  "author": "Kooki-Kodes",
  "repository": {
    "type": "git",
    "url": "https://github.com/KookiKodes/solidifront.git",
    "directory": "packages/plugins/vite-generate-shopify-locales"
  },
  "keywords": [
    "shopify",
    "locales",
    "vite",
    "vite-plugin",
    "vite-plugin-shopify-locales",
    "solidify"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./locales": {
      "import": "./dist/locales/index.js",
      "require": "./dist/locales/index.cjs",
      "types": "./dist/locales/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^5.3.1"
  },
  "dependencies": {
    "vite": "^5.3.1"
  },
  "devDependencies": {
    "@solidifront/codegen": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "node-fetch": "^3.3.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "tsup": "^8.2.4"
  }
}
